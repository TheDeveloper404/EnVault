# Copy to managed-observability.local.env and fill with real values

# Grafana Cloud Loki push URL, e.g. https://logs-prod-XXX.grafana.net/loki/api/v1/push
GRAFANA_LOKI_PUSH_URL=https://logs-prod-xxxx.grafana.net/loki/api/v1/push

# Grafana Cloud credentials (Loki)
GRAFANA_LOKI_USERNAME=123456
GRAFANA_LOKI_API_KEY=glc_xxxxxxxxxxxxxxxxxxxx

# Optional webhook targets used by alert scripts/router (Slack/Discord etc.)
PRIMARY_ALERT_WEBHOOK=https://hooks.slack.com/services/PRIMARY
ONCALL_ALERT_WEBHOOK=https://hooks.slack.com/services/ONCALL
LEADERSHIP_ALERT_WEBHOOK=https://hooks.slack.com/services/LEADERSHIP
