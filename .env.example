# EnVault Environment Variables
# Copy to .env and fill in your values

# Required: 64-char hex or base64 key for encryption
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
ENVALT_MASTER_KEY=aabbccdd11223344aabbccdd11223344aabbccdd11223344aabbccdd11223344

# Optional: API Configuration
PORT=7777
HOST=0.0.0.0
LOG_LEVEL=info

# Optional: Database (SQLite)
DATABASE_URL=file:./envault.db

# Optional: CORS (for development)
CORS_ORIGIN=true

# Optional: CLI Configuration
ENVALT_API_URL=http://localhost:7777
